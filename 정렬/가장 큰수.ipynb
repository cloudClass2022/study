    "def solution(numbers):\n",
    "    a = list(map(lambda num : str(num) * 3, numbers))\n",
    "    a.sort(reverse=True)\n",
    "    \n",
    "    answer = ''\n",
    "    for i in range(len(a)):\n",
    "        answer += a[i][:int(len(a[i])/3)]   \n",
    "    return str(int(answer))"
   ]
  }
